{
  "name": "rabatten-scraper",
  "version": "1.2.0",
  "description": "Used to do web scraping of data for repo rabatten",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "watchFile:index": "nodemon ./index.js",
    "index all": "node ./index.js all",
    "index": "node ./index.js",
    "analyseLastScrape": "node ./test/analyseLastScrape/test.js",
    "release": "release-it --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zinen/rabatten-scraper.git"
  },
  "homepage": "https://github.com/zinen/rabatten-scraper#readme",
  "bugs": {
    "url": "https://github.com/zinen/rabatten-scraper/issues"
  },
  "author": "zinen",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "cheerio": "^1.0.0-rc.5",
    "dotenv": "^8.2.0",
    "node-fetch": "^2.6.1",
    "puppeteer": "^5.5.0"
  },
  "devDependencies": {
    "inquirer": "^7.3.3",
    "release-it": "^14.2.2"
  },
  "nodemonConfig": {
    "ignore": [
      "*.json"
    ]
  },
  "optionalDependencies": {
    "nodemon": "^2.0.7"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    }
  }
}
